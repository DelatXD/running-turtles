CREATE TABLE Aluguel_de_Carros ( ID_Carro varchar (1000) primary KEY UNIQUE, RG_Cliente VARCHAR(100), duração_Temp_Aluguel varchar(30), Total_a_Pagar varchar(10) ); CREATE TABLE Carros ( ID_Carros INT (8) UNIQUE PRIMARY KEY , Modelo VARCHAR(100), Marca varchar(30), Preco DECIMAL(10), Data_de_Fabricação varchar(8) ); CREATE TABLE Cliente ( ID_Cliente INT UNIQUE PRIMARY KEY, Nome_Cliente VARCHAR(100), RG varchar(10), Data_de_nascimento varchar(18), Estado_Civil varchar(9), Renda_Cliente varchar(16) ); CREATE TABLE Funcionarios( ID_Funcionario INT(8) primary KEY UNIQUE, Nome VARCHAR(100), RG varchar(30), Data_de_nascimento varchar(10), funcao_funcionario varchar(69) ); CREATE TABLE Garantia ( ID_Manutenção varchar (9) primary KEY UNIQUE, local_Manutenção VARCHAR(100), Duração_Garantia varchar(30), Preço_Manutenção varchar(10) ); CREATE TABLE Investimento( ID_Investidor INT(8) primary KEY UNIQUE, Nome VARCHAR(100), Investido varchar(30), Data_de_investimento varchar(10) ); CREATE TABLE marketing ( ID_marketing varchar (1000) primary KEY UNIQUE, local_publi VARCHAR(100), investimento_marca varchar(30), durabilidade_anu varchar(10) ); CREATE TABLE Seguro ( ID_seguro varchar (1000) primary KEY UNIQUE, empresa_seguro VARCHAR(100), duração_seguro varchar(30), Preço_seguro varchar(10) ); CREATE TABLE Unidades( ID_Unidade INT(8) primary KEY UNIQUE, Endereço VARCHAR(100), Cidade varchar(30), Vendas_Mensais varchar(10), Quantidade_Funcionarios varchar(9) ); CREATE TABLE Vendas ( ID INT UNIQUE PRIMARY KEY, Nome VARCHAR(100), Preco DECIMAL(10,2), DataVenda DATE); -- INDEX -- TRIGGER -- VIEW
